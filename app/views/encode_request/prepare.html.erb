<%= form_for :post, :url => { :action => :extract } do |f| %>
<p>
<%= f.label :clade %>
<%= f.text_field :clade, :value => "#{@request_params["clade"]}" %>

<%= f.label :org %>
<%= f.text_field :org, :value => "#{@request_params["org"]}" %>
</p>

<p>
<%= f.label :db %>
<%= f.text_field :db, :value => "#{@request_params["db"]}" %>

<%= f.label :hgta_group %>
<%= f.text_field :hgta_group, :value => "#{@request_params["hgta_group"]}" %>
</p>

<p>
<%= f.label :hgta_track %>
<%= f.text_field :hgta_track, :value => "#{@request_params["hgta_track"]}" %>

<%= f.label :hgta_table %>
<%= f.text_field :hgta_table, :value => "#{@request_params["hgta_table"]}" %>
</p>

<p>
<%= f.label :gene_ids %> <br>
<%= f.text_area :gene_ids, :value => "NM_001005288" %>
</p>

<p>
<%= f.label :upstream %>
<%= f.text_field :upstream, :value => "500" %>

<%= f.label :downstream %>
<%= f.text_field :downstream, :value => "500" %>
</p>

<p>
<%= f.submit 'ExtractData' %>
</p>
<% end %>
